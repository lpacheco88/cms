<div class="form-group mb-2">
  <label for="name" class="sr-only">Nome</label>
  <input
    type="text"
    class="register-form-control"
    id="name"
    name="name"
    placeholder="Seu nome completo"
    value="<%= user.name%>"
  />
</div>
<div class="form-group mb-2">
  <label for="email" class="sr-only">Email</label>
  <input
    type="text"
    class="register-form-control"
    id="email"
    name="email"
    value="<%= user.email%>"
    placeholder="email@example.com"
  />
</div>
<div class="form-group  mb-2">
  <label for="password" class="sr-only">Password</label>
  <input
    type="password"
    class="register-form-control"
    id="password"
    name="password"
    value="<%= user.password%>"
    placeholder="Password"
  />
</div>
<div class="form-group  mb-2">
  <label for="usuario" class="sr-only">Usuario</label>
  <input
    type="text"
    class="register-form-control"
    id="usuario"
    name="usuario"
    value="<%= user.usuario%>"
    placeholder="Usuario"
  />
</div>

<div class="form-group  mb-2">
  <label for="cargo" class="sr-only">Cargo</label>
  <input
    type="text"
    class="register-form-control"
    id="cargo"
    name="cargo"
    value="<%= user.cargo%>"
    placeholder="Cargo"
  />
</div>

<div class="form-group  mb-2">
  <label for="dataCadastro" class="sr-only">Data de cadastro</label>
  <input
    type="date"
    class="register-form-control"
    name="dataCadastro"
    id="dataCadastro"
    value="<%= user.dataCadastro == null ? '': user.dataCadastro.toISOString().split('T')[0] %>"
  />
</div>
